alias ls='ls --color=auto'
alias ll="ls -alF"
alias la="ls -A"
alias l="ls -CF"

alias grep='grep --color=auto'
alias fgrep='fgrep --color=auto'
alias egrep='egrep --color=auto'

alias cls="clear"

alias z="zsh"
alias x="exit"

alias omp="eval \"\$(oh-my-posh --init --shell bash --config /usr/local/etc/oh-my-posh/arcan-sh.omp.json)\""
